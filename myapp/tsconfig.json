{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "resolveJsonModule": true,
    "types": ["react-native", "jest"],
    "lib": ["es2020", "dom", "dom.iterable"],
    "target": "es2020"
  },
  "include": ["**/*.ts", "**/*.tsx", "types/**/*.d.ts", "__tests__/**/*.ts", "__tests__/**/*.tsx"],
  "exclude": ["node_modules"]
}
